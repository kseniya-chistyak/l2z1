#include <iostream>

using namespace std ;

float SquaredSum(float a, float b,float result)
{
    result=(a+b)*(a+b);
    return result;
}

int main ()
{
    float c,d,result;
    cout<<"Введите первое дробное число\n";
    cin>>c;
    cout<<"Введите второе дробное число\n";
    cin>>d;
    cout<<"Результат - "<<SquaredSum(c,d,result)<<"\n";
}
