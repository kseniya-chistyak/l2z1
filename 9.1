#include <iostream>
using namespace std;

class str {
private:
string text [200];
};

string operator ++(string text){
for(int i=0; i<text.length(); i++)
text[i] = text[i]+1;
return text;
}
string operator --(string text){
for(int i=0; i<text.length(); i++)
text[i] = text[i]-1;
return text;
}

int main(){
string text = "abc123";
cout << ++text << endl;
cout << --text << endl;
return 0;
}

