#include<fstream>// подключаем библиотеку для ввода и вывода с файла
#include<iostream>
using namespace std;//использование пространство имен std
int main()// точка входа в программу(начало программы)
{
int a, b;//обявляем переменные
ofstream f;//создаем файл 
f.open ("output");//открывем этот же  файл с именем output
cout<<" a = ";//вывод значения а
cin>>a;//ввод значения а
cout<<" b = ";//вывод значения в
cin>>b;//ввод значения а
for (int i = a; i <= b ; i++)//используем цикл for(инициальзация начального значения int i = a,
//задаем условие при каждом шаге цикла i <= b,приращениекак будет изменяться значение в каждом шаге цикла,шаг составляет единицу) )
{
f << i * 3 << endl;//умножениев файле каждого элемента  на 3,перевода текста на новую строку)
}
f.close ( );//закрытие файла
return 0;
}
